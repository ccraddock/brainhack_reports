keywords = {Adult,Aged,Artifacts,Cadaver,Case-Control Studies,Humans,Magnetic Resonance Imaging,Magnetic Resonance Imaging: methods,Male,Middle Aged,Motion,Normal Distribution,Phantoms, Imaging},
month = may,
number = {5},
pages = {526--33},
